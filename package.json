{
  "name": "honeypot",
  "version": "1.0.0",
  "description": "Product for the company Software Skills",
  "main": "gulpfile.js",
  "scripts": {
    "lint": "./node_modules/.bin/jshint . --reporter=./node_modules/jshint-full-path/index.js",
    "jscs": "./node_modules/.bin/jscs .",
    "pretest": "npm run-script lint && npm run-script jscs",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --recursive -R spec -r should",
    "posttest": "./node_modules/.bin/istanbul check-coverage && rm -rf coverage"
  },
  "dependencies": {
    "gulp": "^3.8.10"
  },
  "devDependencies": {
    "connect-livereload": "^0.5.2",
    "express": "^4.11.1",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-minify-css": "^0.4.2",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^1.1.0",
    "istanbul": "~0.2.3",
    "jscs": "~1.3",
    "jshint": "~2.4.1",
    "jshint-full-path": "~1.1.1",
    "mocha": "~1.17.0",
    "node-bourbon": "^1.2.3",
    "node-neat": "^1.4.2",
    "rewire": "~2.0.0",
    "should": "~3.0.1",
    "sinon": "~1.7.3",
    "tiny-lr": "^0.1.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tikkat/honeypot.git"
  },
  "author": "Tobias Tikka, Marcus Olsson, Daniel Bäckström, Niclas Alexandersson, Sebastian Bellevik & John Petersson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tikkat/honeypot/issues"
  }
}
